<p id="notice"><%= notice %></p>

<h1>Race Players</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @race_players.each do |race_player| %>
      <tr>
        <td><%= link_to 'Show', race_player %></td>
        <td><%= link_to 'Edit', edit_race_player_path(race_player) %></td>
        <td><%= link_to 'Destroy', race_player, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Race Player', new_race_player_path %>
